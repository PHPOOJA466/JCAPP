<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
<Dialog title="Attachment List" showHeader="true">
	<List  items="{path:'RecallAttachmentsModel>/results'}">
		<CustomListItem>
			<HBox>
				<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
					<Link text="{RecallAttachmentsModel>documents}"  press="onhandleRecallAttPress"/>
				</VBox>
			</HBox>
		</CustomListItem>
	</List>
	<buttons>
			<Button text="Cancel" press="onRecallAttCancel"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>