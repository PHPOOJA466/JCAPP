<core:FragmentDefinition xmlns="sap.m" xmlns:si="sap.suite.ui.commons.statusindicator" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:core="sap.ui.core">
	<Dialog title="Insurance Search" contentHeight="280px" contentWidth="700px">
		<l:Grid defaultSpan="L12 M12 S12" width="auto">
			<l:content>
				<f:SimpleForm id="insuranceFormID" minWidth="1024" maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" emptySpanL="0"
					emptySpanM="0" columnsL="2" columnsM="2" class="editableForm">
					<f:content>
						<Label text="Insurance Name"/>
						<Input id="insuranceFormName"/>
						<Label text="Address"/>
						<Input value=""/>
						<Label text="Pincode" required="true" />
						<Input value ="" type="Number" liveChange="onChangeInsurancePincode"/>
						<Label text="City"></Label>
						<Input value=""/>
						<Label text="GSTIN"/>
						<Input value=""/>
					</f:content>
				</f:SimpleForm>
			</l:content>
		</l:Grid>
		<buttons>
			<Button text="Search" press="onSubmitInsuranceFormDetails"/>
			<Button text="Cancel" press="OnCancelInsuranceFormDetails"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>
