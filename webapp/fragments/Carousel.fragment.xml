<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
<Dialog id="idpictureviewer" showHeader="false">
	<endButton>
			<Button type="Reject" text="Close" press="viewerclose"/>
	</endButton>
	<Carousel class="sapUiContentPadding" loop="true" pages="{oLocalJsonModel>/ImgModel}" width="350px" height="350px">
        <Image src="{oLocalJsonModel>url}" alt="Picture" />
    </Carousel>
</Dialog>
</core:FragmentDefinition>
